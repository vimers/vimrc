{
	"clangd.arguments": ["-j=1", "--log=error"],
	"git.enableGutters": true,
	"git.addedSign.hlGroup": "GitGutterAdd",
	"git.changedSign.hlGroup": "GitGutterChange",
	"git.removedSign.hlGroup": "GitGutterDelete",
	"git.topRemovedSign.hlGroup": "GitGutterDelete",
	"git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
	"coc.preferences.currentFunctionSymbolAutoUpdate": true,
	"coc.preferences.messageLevel": "error",
	"suggest.noselect": true,
	"suggest.enablePreselect": false,
	"suggest.triggerAfterInsertEnter": true,
	"suggest.autoTrigger": "always",
	"suggest.timeout": 5000,
	"suggest.detailField": "preview",
	"suggest.snippetIndicator": "",
	"suggest.triggerCompletionWait": 50,
	"suggest.completionItemKindLabels": {
		"class": "\uf0e8",
		"color": "\ue22b",
		"constant": "\uf8fe",
		"default": "\uf29c",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"event": "\ufacd",
		"field": "\uf93d",
		"file": "\uf723",
		"folder": "\uf115",
		"function": "\u0192",
		"interface": "\uf417",
		"keyword": "\uf1de",
		"method": "\uf6a6",
		"module": "\uf40d",
		"operator": "\uf915",
		"property": "\ue624",
		"reference": "\ufa46",
		"snippet": "\ue60b",
		"struct": "\ufb44",
		"text": "\ue612",
		"typeParameter": "\uf728",
		"unit": "\uf475",
		"value": "\uf89f",
		"variable": "\ue71b"
	},
	"list.signOffset": 1,
	"diagnostic.errorSign": "\uf467",
	"diagnostic.warningSign": "\uf071",
	"diagnostic.infoSign": "\uf129",
	"diagnostic.hintSign": "\uf864",
	"diagnostic.refreshOnInsertMode": false,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.virtualTextPrefix": " >>> ",
	"diagnostic.virtualText": false,
	"codeLens.enable": true,
	"list.previewHighlightGroup": "Statement",
	"snippets.ultisnips.directories": [
		"$HOME/.config/nvim/UltiSnips/",
		"$HOME/.vim/plugged/vim-snippets/UltiSnips/"
	],
	"snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips",
	"signature.target": "float",
	"prettier.tabWidth": 4,
	"prettier.printWidth": 100,
	"prettier.disableLanguages": [],
	"prettier.formatterPriority": 1,
	"prettier.useTabs": true,
	"prettier.trailingComma": "all",
	"prettier.singleQuote": false,
	"translator.engines": ["bing", "youdao"],
	"coc.source.emoji.filetypes": ["markdown", "gitcommit"],
	"outline.keepWindow": true,
	"outline.autoPreview": false,
	"outline.followCursor": true,
	"explorer.icon.enableNerdfont": true,
    "tabnine.priority": 2
}
